<%- include('header')%>
  
    
  <div class="container mt-5">
  
     <div class="row">
       
            <h3 class="table_main">password Catagory List</h3>
        <table class="table table-striped mt-2">
                <thead>
                    <tr>
                        <th>password catagory name</th>
                       
                        <th>Action</th>
                    </tr>
                </thead>
          
            <tbody class="table table-striped bg-none">
            <% if (records.length>0) {
              records.forEach(function(row){
                %>  
              <tr>
                     <td class="table_body"><%=row.passwordCatagory %></td> 
                    <td >
                        <a class="btn btn-primary btn-xs  buttons" href="/password_catagory/edit/<%=row._id%>"><i class="fa fa-edit"></i></a>     
                        <a class="btn btn-danger btn-xs  buttons" href="/password_catagory/delete/<%=row._id%>"><i class="fa fa-trash"></i></a> 

                    </td>
                 </tr>
                 <% }) }   %>
            </tbody>
        </table>
       
      
    </div>
      
     </div>
 

</body>
</html>