<%- include('header')%>
  
    
 
<div class="container" style="margin-top: 50px;">
     <div class="row">
   
       <div class="col-md-6 offset-md-3">
        
            <div class="card">
              <div class="card-header  text-white text-center"style="background-color:#99CCFF;">
                
                <P style="text-transform: uppercase; color: black;">add new Catagory</P>
              </div>
              <div class="card-body">
                <form method="POST" action="/add_new_catagory">
      
                  <div class="form-group ">
                  <%
                    if(errors!=''){%>
                      <p style="color:red;"><%= errors.passwordcatagory.msg%></p>
                    <%}
                  %>
                   <%
                    if(success!=''){%>
                      <p style="color:green;"><%= success%></p>
                    <%}
                  %>
                    <label for="user">catagory name</label>
                    <input type="text" class="form-control" name="passwordcatagory" id="exampleInputuser1" aria-describedby="userHelp" name="newcatagory" placeholder="add your paddword for new catagory">
                   
                  </div>
                  
               
              
                  <button type="submit" class="btn btn-primary offset-md-3 col-md-6">Add NewCatagort</button>
                  
                </form>
                     <a href="/password_catagory" > view all catagories</a>

              </div>
            </div>
       </div>
     </div>
 </div>

</body>
</html>